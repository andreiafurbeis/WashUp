<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrearubeis.wash_up">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/wash_up_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.andrearubeis.wash_up.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.andrearubeis.wash_up.Registrazione" />
        <activity
            android:name="com.example.andrearubeis.wash_up.bottom_activity"
            android:label="@string/title_activity_bottom_activity" />
        <activity android:name="com.example.andrearubeis.wash_up.NewHome"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.AggiungiStanzaActivity"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.ConfigurazioneStanzaActivity"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.RuoliStanza"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.ModificaCompitiActivity"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.AggiungiInquilinoActivity"></activity>
        <activity android:name="com.example.andrearubeis.wash_up.RuoliInquilino"></activity>

    </application>

</manifest>